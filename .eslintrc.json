{
    "extends": ["airbnb", "prettier"],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "settings": {
      "import/parser": "babel-eslint"
    },
    "rules": {
      "import/prefer-default-export": "off",
      "react/forbid-prop-types": "off",
      "react/react-in-jsx-scope": "off",
      "max-len": "off",
      "react/jsx-props-no-spreading": [
        "error",
        {
          "custom": "ignore"
        }
      ]
    }
  }
  